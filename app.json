{
  "name": "Megajs CDN",
  "description": "A lightweight and serverless CDN utilizing MEGA for file storage and delivery...",
  "logo": "https://files.catbox.moe/52699c.jpg",
  "repository": "https://github.com/mouricedevs/mega-cdn",
  "keywords": ["Gifted-Tech"],
  "success_url": "/",
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-nodejs#latest"
    }
  ],
  "env": {
    "PORT": {
      "description": "Port for web app. 4000, 5000, 3000... any!",
      "value": "5000",
      "required": true
    },
    "EMAIL": {
      "description": "Email for MegaJS authentication.",
      "value": "",
      "required": true
    },
    "PASS": {
      "description": "Password for MegaJS authentication.",
      "value": "",
      "required": true
    },
    "MONGO_URI": {
      "description": "MongoDB Connection URI.",
      "value": "",
      "required": true
    }
  }
}
